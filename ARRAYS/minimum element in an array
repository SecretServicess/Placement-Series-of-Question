// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int minielement(int arr[], int n){
    
    int mini = arr[0];
    for(int i=1;i<n;i++){
        if(mini>arr[i]){
            mini = arr[i];
        }
    }
    return mini;
}
int main() {
    int n;
    cout<<"Enter the valuse of n:"<<" ";
    cin>>n;
    
    int  arr[n];
    cout<<"Enter the value:"<<endl;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    int result = minielement(arr,n);
    cout<<"the answer is "<<result<<endl;
    

    return 0;
}
