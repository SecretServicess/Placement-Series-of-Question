void rotate(int arr[], int n) {
// Store the last element in a temporary variable
    int lastElement = arr[n - 1]; 

    for (int i = n - 1; i > 0; i--) {

// Shift elements to the right
        arr[i] = arr[i - 1]; 

    }

    arr[0] = lastElement; // Place the last element in the first position
}
