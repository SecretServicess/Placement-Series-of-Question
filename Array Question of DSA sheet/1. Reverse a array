#include<bits/stdc++.h>
using namespace std;

class Solution{
    public:
    string reverseWord(string str){
        
        int n = str.size();
        
        int start = 0;
        int end = n-1;
        
        
        while(start<end){
            swap(str[start],str[end]);
            start++;
            end--;
        }
        return str;
    }
};
